<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.rudraambition.ultimatetextscanner">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/mainpic"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/mainpic"
        android:supportsRtl="true"
        tools:replace="android:icon"
        android:theme="@style/AppTheme">
        <activity android:name="com.rudraambition.ultimatetextscanner.About_Us"
            android:parentActivityName="com.rudraambition.ultimatetextscanner.HomeActivity"></activity>

        <activity android:name="com.rudraambition.ultimatetextscanner.HomeActivity" />
        <activity
            android:name="com.rudraambition.ultimatetextscanner.MainActivity2"
            android:parentActivityName="com.rudraambition.ultimatetextscanner.HomeActivity" />
        <activity
            android:name="com.rudraambition.ultimatetextscanner.MainActivity"
            android:noHistory="true"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.theartofdev.edmodo.cropper.CropImageActivity"
            android:theme="@style/Base.Theme.AppCompat" />

        <meta-data android:name="applovin.sdk.key" android:value="rMiBQFNASEqb3y7phspvbxBzMR1KzIdc1_EFhe6LTU6JrspkhHr-StGpNS1yIRofTJUWOVWtw2J3c5yT7ipKHa"
        />
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-2507491428423834~6645216741" />

    </application>

</manifest>